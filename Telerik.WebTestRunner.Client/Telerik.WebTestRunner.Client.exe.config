  <credentialsConfiguration>
    <tfisSettings enabled="false" tokenEndpoint="https://uatidentity.telerik.com/v2/oauth/telerik/token" basicAuthorization="dXJpJTNBaW50ZWdyYXRpb24udGVzdHM6NDcwNzE5MTU0NjZmYTBlNWYwNmRlYWQ3NGY4MTFkMzE="/>
    <credentials>
      <credential username="admin@test.test" password="admin@2" provider="" isActive="true"/>
